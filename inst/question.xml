<item ident="#IdentityQuestion" title="Question">
  <itemmetadata>
    <qtimetadata>
      <qtimetadatafield>
        <fieldlabel>question_type</fieldlabel>
        <fieldentry>multiple_choice_question</fieldentry>
      </qtimetadatafield>
      <qtimetadatafield>
        <fieldlabel>points_possible</fieldlabel>
        <fieldentry>1.0</fieldentry>
      </qtimetadatafield>
      <qtimetadatafield>
        <fieldlabel>original_answer_ids</fieldlabel>
        <fieldentry>#respid1,#respid2,#respid3,#respid4</fieldentry>
      </qtimetadatafield>
      <qtimetadatafield>
        <fieldlabel>assessment_question_identifierref</fieldlabel>
        <fieldentry>#IdentityQuestion</fieldentry>
      </qtimetadatafield>
    </qtimetadata>
  </itemmetadata>
  <presentation>
    <material>
      <mattext texttype="text/plain">#QuestionText</mattext>
    </material>
    <response_lid ident="response1" rcardinality="Single">
      <render_choice>
        <response_label ident="#respid1">
          <material>
            <mattext texttype="text/plain">#Choice1Text</mattext>
          </material>
        </response_label>
        <response_label ident="#respid2">
          <material>
            <mattext texttype="text/plain">#Choice2Text</mattext>
          </material>
        </response_label>
        <response_label ident="#respid3">
          <material>
            <mattext texttype="text/plain">#Choice3Text</mattext>
          </material>
        </response_label>
        <response_label ident="#respid4">
          <material>
            <mattext texttype="text/plain">#Choice4Text</mattext>
          </material>
        </response_label>
      </render_choice>
    </response_lid>
  </presentation>
  <resprocessing>
    <outcomes>
      <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
    </outcomes>
    <respcondition continue="Yes">
      <conditionvar>
        <other/>
      </conditionvar>
      <displayfeedback feedbacktype="Response" linkrefid="general_fb"/>
    </respcondition>
    <respcondition continue="No">
      <conditionvar>
        <varequal respident="response1">#respidcorrect</varequal>
      </conditionvar>
      <setvar action="Set" varname="SCORE">100</setvar>
    </respcondition>
  </resprocessing>
  <itemfeedback ident="general_fb">
    <flow_mat>
      <material>
        <mattext texttype="text/plain">#GeneralFeedbackText</mattext>
      </material>
    </flow_mat>
  </itemfeedback>
</item>
